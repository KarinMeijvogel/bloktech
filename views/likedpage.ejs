    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="../static/styles/likedpage.css">
        <title>Liked people</title>
    </head>
    <body>
        <header>
            <h1>Liked people</h1>
        </header>
        
        <main>
            <ul>
                <% for (let i = 0; i < data.length; i++) { %>
                    <li>
                        <figure>
                            <img data-id="<%= data[i].id %>" src="../static/images/<%= data[i].photo %>" alt="Profilepicture">
                            <figcaption>
                                <h4><%= data[i].firstName %></h4>
                                <p><%= data[i].msg %></p>
                            </figcaption>
                            <button>X</button>
                        </figure>
                    </li>
                <% } %>
            </ul>
        </main>

        <footer>
            <p>No more people in your liked list.</p>
        </footer>
                    
        <%- include ('nav') -%>

        <script src="../static/scripts/likedpage.js"></script>
    </body>
    </html>